{
  "plugins": ["prettier-plugin-solidity"],
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": false,
  "semi": true,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 120,
        "tabWidth": 4,
        "explicitTypes": "always",
        "semi": true
      }
    }
  ]
}
